<%= form_with model: model, method: method, url: url, local: true do |f| %>
  <%= f.label :percentage %>
  <%= f.text_field :percentage, value: percent_default %><br/><br/>
  <%= f.label :quantity_threshold %>
  <%= f.text_field :quantity_threshold, value: quantity_default %><br/><br/>
  <%= f.submit 'Submit'%>
<% end %>